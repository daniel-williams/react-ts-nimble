{
  "name": "react-ts-nimble",
  "version": "1.0.0",
  "description": "React starter with typescript, babel, webpack, and testing.",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "postinstall": "npm run typings",
    "typings": "rimraf typings/ && typings install"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start config/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "keywords": [
    "react",
    "typescript",
    "babel",
    "webpack",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-williams/react-ts-nimble.git"
  },
  "author": "Daniel James Williams",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "better-npm-run": "0.0.11",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "codecov": "^1.0.1",
    "css-loader": "^0.24.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.11.0",
    "postcss-modules-local-by-default": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.1",
    "redbox-react": "^1.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-webpack-plugin": "^0.3.1",
    "ts-helpers": "^1.1.1",
